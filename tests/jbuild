(jbuild_version 1)

(executables
 ((names (capabilities version vertexarrays graphs programs))
  (libraries (ogaml.graphics))))

(alias
 ((name    runtest)
  (deps    (version.exe))
  (action  (run ${<}))))

(alias
 ((name    runtest)
  (deps    (vertexarrays.exe))
  (action  (run ${<}))))

(alias
 ((name    runtest)
  (deps    (graphs.exe))
  (action  (run ${<}))))

(alias
 ((name    runtest)
  (deps    (capabilities.exe))
  (action  (run ${<}))))

(alias
 ((name    runtest)
  (deps    (programs.exe))
  (action  (run ${<}))))

